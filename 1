#include <iostream>
using namespace std;

bool isPossible(int n) {
    if (n == 1) {
        return true;
    }
    else if (n < 1) {
        return false;
    }
    else {
        return isPossible(n - 3) || isPossible(n - 5);
    }
}

int main() {
    int n;
    cin >> n;
    if (isPossible(n)) {
        cout << "YES" << endl;
    }
    else {
        cout << "NO" << endl;
    }
    return 0;
}
