#include <iostream>
using namespace std;

int findMaxDigit(string str, int index, int maxDigit) {
    
    if (index == str.length()) {
        return maxDigit;
    }

    int digit = str[index] - '0';

    if (digit > maxDigit) {
        maxDigit = digit;
    }

    return findMaxDigit(str, index + 1, maxDigit);
}

int main() {
    string str;
    cin >> str;

    int maxDigit = -1;

    int result = findMaxDigit(str, 0, maxDigit);

    cout << result << endl;

    return 0;
}
